bb = 0
gg = 0



def InstaGram(user, proxy):
      global gg, bb
      try:
          lsd = ''.join(random.choices(ssc + dd, k=12))
          em = "".join(random.choice('1234567890qwertyuiopasdfghjklzxcvbnm.') for _ in range(int("".join(random.choice("6789") for _ in range(1)))))
          funame = faker.name()
          targetName = faker.name()
          url = "https://help.instagram.com/ajax/help/contact/submit/page"
