M = '\x1b[1;37m'
B = '\x1b[38;5;208m'
memo = random.randint(100, 300)
O = f'\x1b[38;5;{memo}m'

def nx():
    os.system("clear")
    Banner = f"""{B}{E}=============================={B}
|{F}[+] YouTube    : {B} नही है ।।
|{F}[+] TeleGram   : {B} Mard_bihari    
|{F}[+] Instagram  : {B} cxmsrial
|{F}[+] Tool  : {B}Available Username IG (Checker) |
|{F}[+] sever  : {B} Web |
{E}==============================
"""
    for mm in Banner.splitlines():
        time.sleep(0.05)
        print(mm)

nx()

